<script src="http://yui.yahooapis.com/3.10.1/build/yui/yui-min.js"></script>
<script type="text/javascript">
    (function (exports) {
        var _d = exports.document, _n = exports.navigator;
        var _warn = document.getElementById("warn");
        var x, y, local, map;

        function getLocation() {
            if (_n.geolocation) {
                _n.geolocation.getCurrentPosition(showPosition);
            }
            else {
                _warn.innerHTML = "Geolocation is not supported by this browser.";
            }
        }

        function showPosition(position) {
            x = position.coords.latitude;
            y = position.coords.longitude;
            map = new BMap.Map("allmap");
            var _point = new BMap.Point(y, x);
            map.centerAndZoom(_point, 14);
            local = new BMap.LocalSearch(map, {
                renderOptions:{map:map}
            });
            map.addOverlay(new BMap.Marker(_point));
            showWether(x, y);
            /* local.searchInBounds("银行", map.getBounds());

             map.addEventListener("dragend", function () {
             map.clearOverlays();
             local.searchInBounds("银行", map.getBounds());
             });*/
        }

        getLocation();
        var _cur = document.getElementById("cur");
        var _res = document.getElementById("res");
        var _enter = document.getElementById("enter");
        var _fp = document.getElementById("_fp");
        _cur.onclick = function () {
            window.scrollTo(0, 0);
            getLocation();
        };
        _res.onclick = function () {
            //getLocation();
            window.scrollTo(0, 0);
            map.reset();
            local.searchInBounds("餐饮", map.getBounds());
            map.addEventListener("dragend", function () {
                map.reset();
                map.clearOverlays();
                local.searchInBounds("餐饮", map.getBounds());
            });
        };
        _enter.onclick = function () {
            //getLocation();
            window.scrollTo(0, 0);
            map.reset();
            local.searchInBounds("娱乐", map.getBounds());
            map.addEventListener("dragend", function () {
                map.reset();
                map.clearOverlays();
                local.searchInBounds("娱乐", map.getBounds());
            });
        };

        var flag = 0;
        var flag1 = 0;
        $("#old_message").click(function () {
            if (flag1 == 0) {
                $.get("oldmessage", {x:x, y:y}, function (data) {
                    flag1=1;
                    var keys = data.key;
                    var html = "";
                    for (var i in keys) {
                        html += "<li><span>" + keys[i].message + "</span><span style='float: right;'>" + keys[i].time + "    发布</span></li>"
                    }
                    $("#old_mess_list").html(html).slideDown();

                });
            }else{
                $("#old_mess_list").slideUp();
                flag1=0;
            }
        });
        $("#post_message").click(function () {
            if (flag == 0) {
                flag = 1;
                $(this).find('.list-text').html("收起留言");
                $("#form").slideDown(function () {
                    window.scrollBy(0, 50);
                });
            } else {
                $("#form").slideUp();
                $(this).find('.list-text').html("我要留言");
                flag = 0;
            }

        });
        $("#submit").on('click', function () {
            $.get("postmessage", {x:x, y:y, message:$("#message").val()}, function () {
                window.scrollTo(0, 0);
                $("#banner").slideDown();
                $("#form").slideUp();
                $("#message").val("");
                $("#post_message").find('.list-text').html("我要留言");
                setTimeout(function () {
                    $("#banner").slideUp()
                }, 3000)
            })
        });
        function showWether(x, y) {
            YUI().use('yql', 'node', function (Y) {
                Y.YQL('select * from weather.forecast where woeid in (select woeid from geo.placefinder where text="' + x + ',' + y + '" and gflags="R")', function (r) {
                    console.log(r);
                    var forcast = r.query.results.channel.item;
                    var title = forcast.title;
                    document.getElementById("w_ds").innerHTML=title;
                    document.getElementById("w_ds").href = r.query.results.channel.link;
                    document.getElementById("date1").innerHTML = forcast.forecast[0].date + " <b>" + forcast.forecast[0].low + " ~ " + forcast.forecast[0].high + "</b>";
                    document.getElementById("date2").innerHTML = forcast.forecast[1].date + " <b>" + forcast.forecast[1].low + " ~ " + forcast.forecast[1].high + "</b>";
                });
            });

        }


    })(window);
</script>
